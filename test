pip install nltk
pip install textblob
pip install newspaper3k

from textblob import TextBlob
from newspaper import Article

url = "https://www.britannica.com/place/South-Korea/Economic-and-social-developments"
article = Article(url)

article.download
article.parse
article.nlp

text = article.text
print(text)

blob = TextBlob(text)
sentiment = blob.sentiment.polarity
print(sentiment)

